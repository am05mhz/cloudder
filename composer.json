{
    "name": "am05mhz/cloudder",
    "description": "Cloudinary API wrapper for Laravel",
    "keywords": ["laravel", "laravel5", "cloudinary", "api", "image", "upload"],
    "homepage": "https://github.com/am05mhz/cloudder",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeremy Dagorn",
            "email": "jeremy.dagorn@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "cloudinary/cloudinary_php": "^2.0"
    },
    "require-dev": {
        "laravel/framework": ">=5.0",
        "phpunit/phpunit": "4.*",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-0": {
            "JD\\Cloudder": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "JD\\Cloudder\\CloudderServiceProvider"
            ],
            "aliases": {
                "Cloudder": "JD\\Cloudder\\Facades\\Cloudder"
            }
        }
    }
}
